{
  "defaultSeverity": "error",
  "extends":         [
    "tslint:latest"
  ],
  "jsRules":         {},
  "rules":           {
    "semicolon":                false,
    "no-console":               false,
    "member-access":            false,
    "object-literal-sort-keys": false,
    "ordered-imports":          false,
    "interface-name":           false,
    "no-submodule-imports":     false,
    "arrow-parens":             false,
    "import-spacing":           false
  },
  "rulesDirectory":  []
}
